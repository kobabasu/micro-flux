{
  "name": "flux-app",
  "version": "0.0.1",
  "description": "flux minimum app",
  "main": "js/app.js",
  "repository": {
    "type": "git",
    "url": "//github.com/kobabasu/micro-flux  .git"
  },
  "keywords": [
    "es6",
    "react",
    "flux"
  ],
  "os": [
    "darwin",
    "linux",
    "!win32"
  ],
  "dependencies": {
    "react": "*",
    "object-assign": "*",
    "flux": "*"
  },
  "devDependencies": {
  },
  "browser": {
  },
  "browserify": [ "js", "jsx" ],
  "eslintConfig": {
    "parser": "babel-eslint"
  },
  "scripts": {
    "preinstall": "npm install -g karma",
    "start": "NODE_ENV=production NODE_PATH=js src dist",
    "babel": "babel ./src --out-dir ./js --blacklist js,dist,sample",
    "browserify": "browserify -d --compact false ./js/app.js > ./dist/build.js",
    "build": "npm run babel; npm run browserify"
  },
  "preferGlobal": true,
  "author": "kobabasu",
  "license": "MIT"
}
